/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here: 
    Not(in = a, out = c);
    Not(in = b, out = d);
    Nand(a=c, b=d, out = z);
    Nand(a=a, b=b, out = q);
    And(a=z, b=q, out = out);
    

}